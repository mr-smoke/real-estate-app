{
  "version": 2,
  "builds": [
    {
      "src": "backend/package.json",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "npm run build",
        "startCommand": "npm start"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/.strapi/client/$1"
    }
  ]
}